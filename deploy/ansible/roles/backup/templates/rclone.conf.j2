# Rclone configuration for remote backup
# Generated by Ansible - do not edit manually

{% if backup_b2_enabled | default(false) %}
[b2]
type = b2
account = {{ backup_b2_account_id }}
key = {{ backup_b2_application_key }}
{% endif %}

{% if backup_gdrive_enabled | default(false) %}
[gdrive]
type = drive
scope = drive.file
token = {{ backup_gdrive_token }}
{% endif %}
