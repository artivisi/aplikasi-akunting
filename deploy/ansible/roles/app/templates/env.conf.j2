# Sensitive environment variables for {{ app_name }}
# This file is loaded by systemd EnvironmentFile directive
# Mode: 0600 (owner read/write only)
# DO NOT commit actual values to version control

# Document encryption key (AES-256, 32 bytes Base64 encoded)
# Generate with: openssl rand -base64 32
{% if document_encryption_key is defined and document_encryption_key | length > 0 %}
APP_ENCRYPTION_KEY={{ document_encryption_key }}
{% endif %}
